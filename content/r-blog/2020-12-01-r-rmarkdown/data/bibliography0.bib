@article{Nelson2014a,
abstract = {This issue of Agricultural Economics is a special issue containing$\backslash$narticles on model performance in assessing the effects of climate$\backslash$nchange, bioenergy policy, and socioeconomics on agriculture. The$\backslash$ncontributions present results from a global economic model intercomparison$\backslash$nactivity undertaken as part of the AgMIP Project (www.agmip.org).$\backslash$nThe origins of the comparison activities can be traced to a project$\backslash$nthat was organized by the OECD in late 2010 to compare results from$\backslash$nthree models. The current phase of the research includes 10 models$\backslash$nand was designed in part to support of the IPCC fifth assessment$\backslash$nreport (AR5). The special issue includes seven peer-reviewed articles$\backslash$nthat present thematic results from a range of modeling strategies,$\backslash$nwith partial and general equilibrium modeling as a high level distinction$\backslash$nbut a myriad of differences within these two model types. A central$\backslash$ncommon element is harmonization on biophysical effects using crop$\backslash$nmodels and socioeconomic effects using drivers from the Shared Socioeconomic$\backslash$nPathways developed as part of the AR5 process. The special issue$\backslash$nprovides broad insights into how the modeling communities approached$\backslash$nthe interactions of climate, socioeconomics, bioenergy policy on$\backslash$nagricultural outcomes, including land use, prices, consumption, and$\backslash$nproduction.},
author = {Nelson, Gerald C. and Shively, Gerald E.},
doi = {10.1111/agec.12093},
